<html>
  <head>
    <title>Room Render</title>
    <link rel="stylesheet" href="/global.css">
  </head>
  <body>
    <% for (let i = 0; i<tasks.length; i++) { %>
      <div class="task">
        <div class="header">
          <h1><%- tasks[i].taskTitle %></h1>
        </div>
        <div class="desc">
          <h3><%- tasks[i].taskDesc %></h3>
        </div>       
      </div>
    <% } %>
  </body>
</html>
